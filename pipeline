pipeline{
  agent any
    stages{
      stage('first job'){
       steps{
         echo 'this is first build'
            }
       stage('second job'){
           steps{
            echo 'this is second build'
           }
        stage('hello world'){
           steps{
           echo 'this is third build'
           }
           
           }  
        
          }
        
      
      }
    
    
    
    
    
    
    
    }

}
